{
  "name": "@lodybo/athena",
  "version": "0.0.0",
  "description": "My own React- and Tailwind-based design system",
  "main": "dist/main.cjs.js",
  "module": "dist/main.esm.js",
  "browser": "dist/main.umd.js",
  "files": ["dist"],
  "distributionDirectory": "dist",
  "scripts": {
    "start": "clear && npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build": "npm run build:rollup && npm run build:storybook",
    "build:rollup": "rollup --config",
    "build:watch": "npm run build:rollup -- -w",
    "build:storybook": "build-storybook -o athena-storybook",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "plop",
    "prerelease": "npm run build",
    "release": "np",
    "postrelease": "git push && git push --tags",
    "release:beta": "npm run release prerelease -- --tag=next"
  },
  "keywords": [
    "React",
    "Storybook",
    "Tailwind",
    "Design System"
  ],
  "author": "Lody Borgers <hi@lodybo.nl>",
  "repository": {
    "type": "git",
    "url": "https://github.com/lodybo/athena"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.2.9",
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.9",
    "autoprefixer": "^10.2.5",
    "classnames": "^2.2.6",
    "np": "^7.5.0",
    "plop": "^2.7.4",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "rollup": "^2.40.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "tailwindcss": "^2.0.3",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@fontsource/bungee": "^4.2.2",
    "@fontsource/bungee-shade": "^4.2.2",
    "@fontsource/roboto-mono": "^4.2.2",
    "@fontsource/roboto-slab": "^4.2.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14"
  }
}
